{
  "name": "solid-pane-scratchpad",
  "version": "0.0.1",
  "main": "dist/main.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest",
    "watch": "concurrently --names webpack,tsc \"webpack --watch\" \"tsc --watch --emitDeclarationOnly\"",
    "build-dev": "npm run clean; webpack && tsc --emitDeclarationOnly",
    "build": "npm run clean; webpack --mode=production && tsc --emitDeclarationOnly"
  },
  "keywords": [
    "solid",
    "solid-pane"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.13",
    "@types/webpack-env": "^1.13.9",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "concurrently": "^4.1.0",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5",
    "webpack": "^4.32.1",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4"
  }
}
